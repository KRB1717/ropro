    <!-- Diff Drive Plugin -->
    <plugin name="diff_drive_front" filename="libgazebo_ros_diff_drive.so">
      <ros>
        <namespace>/</namespace>
        <remapping>cmd_vel:=/cmd_vel</remapping>
      </ros>
      <robotNamespace>my_robot</robotNamespace>
      <left_joint>joint_fl</left_joint>
      <right_joint>joint_fr</right_joint>
      <wheel_separation>0.3</wheel_separation>
      <wheel_diameter>0.2</wheel_diameter>
      <maxTorque>10.0</maxTorque>
      <maxVelocity>10.0</maxVelocity>
      <topic>/cmd_vel</topic>
      <odometryFrame>odom</odometryFrame>
      <odometryTopic>/odom</odometryTopic>
    </plugin>

    <plugin name="diff_drive_rear" filename="libgazebo_ros_diff_drive.so">
