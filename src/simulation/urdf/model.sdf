      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>.1</radius>
            <length>.05</length>
          </cylinder>
        </geometry>
      </visual>
    </link>

    <!-- Joint_rear_right -->
    <joint name="joint_rr" type="revolute">
      <pose>0 0 -.025 0 0 0</pose>
      <parent>chassis</parent>
      <child>rear_right_wheel</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <!-- front left Wheel Link -->
    <link name="front_left_wheel">
      <pose>-.2 .175 .1 1.5708 0 0</pose>
      <!-- 질량 및 관성 요소 추가 -->
